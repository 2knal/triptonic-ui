{
  "expo": {
    "name": "triptonic-ui",
    "slug": "triptonic-ui",
    "scheme": "one.triptonic",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/triptonic-logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/triptonic-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F5ECE9"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/triptonic-logo.png",
        "backgroundColor": "#F5ECE9"
      }
    },
    "web": {
      "favicon": "./assets/triptonic-logo.png",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": ["./assets/fonts/bricolage/bricolage.ttf", "./assets/fonts/rethink/rethink.ttf", "./assets/fonts/rethink/rethink-italic.ttf"]
        }
      ]
    ]
  }
}
